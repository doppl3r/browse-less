{
  "name": "Browse Less",
  "description": "Browse less, see more.",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "www/index.html",
    "default_title": "Browse Less"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "run_at": "document_end",
    "js": ["www/js/contentscript.js"]
  }],
  "web_accessible_resources": ["www/js/script.js"],
  "icons": { 
    "16": "www/img/icons/icon16.png", 
    "48": "www/img/icons/icon48.png", 
    "128": "www/img/icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "identity"
  ]
}